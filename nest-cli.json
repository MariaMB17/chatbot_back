{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app-chat-bot/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/app-chat-bot/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/app-chat-bot",
  "projects": {
    "app-chat-bot": {
      "type": "application",
      "root": "apps/app-chat-bot",
      "entryFile": "main",
      "sourceRoot": "apps/app-chat-bot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-chat-bot/tsconfig.app.json"
      }
    },
    "auth-app": {
      "type": "application",
      "root": "apps/auth-app",
      "entryFile": "main",
      "sourceRoot": "apps/auth-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-app/tsconfig.app.json"
      }
    },
    "payment-app": {
      "type": "application",
      "root": "apps/payment-app",
      "entryFile": "main",
      "sourceRoot": "apps/payment-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-app/tsconfig.app.json"
      }
    },
    "chat-logs": {
      "type": "application",
      "root": "apps/chat-logs",
      "entryFile": "main",
      "sourceRoot": "apps/chat-logs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-logs/tsconfig.app.json"
      }
    }
  }
}